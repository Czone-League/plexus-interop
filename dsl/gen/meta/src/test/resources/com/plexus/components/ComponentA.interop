import "com/plexus/services/services.proto";
import "com/plexus/model/options.proto";

package com.plexus.components;

application ComponentA {

        consumes com.plexus.services.ExampleService { PointToPoint; ServerStreaming [(model.consumed_method_option_enum)=VALUE2]; ClientToServer; BidiStreaming; }

        provides com.plexus.services.ExampleService to com.plexus.* { 
        	PointToPoint [launch_on_call=ALWAYS, title="Point-to-point"]; 
        	ServerStreaming; 
        	ClientToServer; 
        	BidiStreaming [launch_on_call=DISABLED]; 
        }
}