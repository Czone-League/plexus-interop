{
  "name": "@plexus-interop/e2e",
  "version": "0.1.2",
  "types": "dist/main/src/index.d.ts",
  "main": "dist/main/src/index.js",
  "private": true,
  "description": "Integration tests for Plexus Web Interop",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "trash dist",
    "build": "yarn build:main && npm run copy-assets && npm run build-host-proxy",
    "build:main": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "lint": "tslint src/**/*.ts",
    "unit": "yarn build && jest --coverage",
    "pretest": "yarn lint",
    "test": "yarn unit",
    "gen-echo-js": "pbjs --force-long -t static-module -r echo-client -w commonjs -o src/echo/gen/plexus-messages.js src/echo/metadata/messages.proto",
    "gen-echo-ts": "pbts --force-long -o src/echo/gen/plexus-messages.d.ts src/echo/gen/plexus-messages.js",
    "gen-echo-protocol": "npm run gen-echo-js && npm run gen-echo-ts",
    "unpublish": "npm unpublish @plexus-interop/e2e@0.1.2",
    "postunpublish": "echo 'Unpublished'",
    "prerepublish": "echo 'Republishing started...'",
    "copy-assets": "copyfiles src/echo/gen/* dist/main && copyfiles src/views/* dist/main",
    "publish-package": "npm run build && npm publish",
    "republish": "npm run unpublish && npm i && npm run publish-package",
    "postrepublish": "echo 'Package republished successfully!'",
    "build-host-proxy": "node scripts/browserify-all.js --standalone proxyHost --inputGlob dist/main/tests/common/TestCrossDomainHost.js --outputFile dist/main/tests/web/CrossDomainHost.bundle.js",
    "prepare-native-e2e-debug-tests": "node scripts/browserify-all.js --inputGlob dist/main/tests/native/*spec.js --outputFile dist/main/tests/native/RendererProcess.bundle.js",
    "e2e-web-electron": "node scripts/web-e2e-tests-launcher.js --file ./dist/main/tests/web/RendererProcess.bundle.js",
    "pree2e-web-electron": "node scripts/browserify-all.js --brokerCoverage true --inputGlob dist/main/tests/web/*spec.js --outputFile dist/main/tests/web/RendererProcess.bundle.js",
    "e2e-web-ie": "node scripts/web-e2e-tests-launcher.js --browser ie",    
    "web-broker-benchmarks": "node scripts/web-e2e-tests-launcher.js --file ./dist/main/tests/web/benchmarks/RendererProcessBenchmarks.bundle.js",
    "preweb-broker-benchmarks": "node scripts/browserify-all.js --inputGlob dist/main/tests/web/benchmarks/*spec.js --outputFile dist/main/tests/web/benchmarks/RendererProcessBenchmarks.bundle.js",
    "native-broker-benchmarks": "node scripts/native-e2e-tests-launcher.js --file ./dist/main/tests/native/benchmarks/RendererProcessBenchmarks.bundle.js",
    "prenative-broker-benchmarks": "node scripts/browserify-all.js --inputGlob dist/main/tests/native/benchmarks/*spec.js --outputFile dist/main/tests/native/benchmarks/RendererProcessBenchmarks.bundle.js",
    "e2e-native": "npm run prepare-native-e2e-tests && node scripts/native-e2e-tests-launcher.js --file ./dist/main/tests/native/RendererProcess.bundle.js",
    "pree2e-native": "node scripts/browserify-all.js --clientCoverage true --inputGlob dist/main/tests/native/*spec.js --outputFile dist/main/tests/native/RendererProcess.bundle.js",
    "e2e-native-debug": "npm run build && npm run prepare-native-e2e-debug-tests && node scripts/native-e2e-tests-launcher.js --printBrokerStdout true --debug true --file ./dist/main/tests/native/RendererProcess.bundle.js"
  },
  "dependencies": {
    "@types/long": "^3.0.31",
    "@plexus-interop/websocket-transport": "0.1.2",
    "@plexus-interop/broker": "0.1.2",
    "@plexus-interop/client": "0.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.0.2",
    "@types/mocha": "^2.2.43",
    "@types/node": "^7.0.5",
    "babel-register": "^6.23.0",
    "browserify": "^14.5.0",
    "browserify-istanbul": "^3.0.1",
    "chai": "^4.1.0",
    "chokidar": "^1.7.0",
    "copyfiles": "^1.2.0",
    "electron": "1.7.5",
    "electron-mocha": "^4.0.2",
    "glob": "^7.1.2",
    "jasmine-reporters": "^2.2.0",
    "jest": "^21.0.1",
    "karma": "^1.7.1",
    "karma-browserify": "^5.1.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "minimist": "^1.2.0",
    "mocha": "^3.5.0",
    "nyc": "^11.2.1",
    "path": "^0.12.7",
    "sinon": "^2.4.1",
    "strip-bom": "^3.0.0",
    "trash-cli": "^1.4.0",
    "tree-kill": "^1.1.0",
    "ts-mockito": "2.0.2",
    "tslint": "^4.5.1",
    "typescript": "2.6.1",
    "watchify": "^3.9.0",
    "yarn": "^1.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jasmine.js",
    "transform": {
      ".(ts|tsx)": "<rootDir>/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": ".+/dist/main/tests/.+spec.js$",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "dist/main/src/**/*.js",
      "!dist/main/src/gen/**/*.js",
      "!src/*.d.ts",
      "!src/**/*.d.ts",
      "!src/**/*.spec.ts"
    ]
  }
}
