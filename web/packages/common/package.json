{
  "name": "@plexus-interop/common",
  "version": "0.0.1",
  "types": "dist/main/src/index.d.ts",
  "main": "dist/main/src/index.js",
  "description": "Common functionality for Plexus Interop Web",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "trash dist && yarn lint",
    "build": "yarn build:main",
    "build:main": "tsc -p tsconfig.json",
    "lint": "tslint src/**/*.ts",
    "unit": "yarn build && jest --coverage",
    "pretest": "yarn lint",
    "test": "yarn unit",
    "unpublish": "npm unpublish @plexus-interop/common@0.0.1",
    "postunpublish": "echo 'Unpublished'",
    "prerepublish": "echo 'Republishing started...'",
    "publish-package": "npm run build && npm publish",
    "republish": "npm run unpublish && npm i && npm run publish-package",
    "postrepublish": "echo 'Package republished successfully!'"
  },
  "dependencies": {
    "@types/loglevel": "^1.4.29",
    "@types/long": "^3.0.31",
    "loglevel": "^1.4.1",
    "loglevel-plugin-prefix": "^0.5.3",
    "long": "^3.2.0",
    "reflect-metadata": "^0.1.10",
    "tslib": "^1.6.0",
    "typescript-collections": "^1.2.3"
  },
  "devDependencies": {
    "@types/chai": "^4.0.2",
    "@types/jest": "^18.1.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.5",
    "chai": "^4.1.0",
    "jest": "^21.0.1",
    "karma": "^1.7.0",
    "path": "^0.12.7",
    "sinon": "^2.4.1",
    "trash": "^4.1.0",
    "trash-cli": "^1.4.0",
    "ts-mockito": "2.0.2",
    "tslint": "^4.5.1",
    "typescript": "~2.3.4",
    "yarn": "^1.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jasmine.js",
    "transform": {
      ".(ts|tsx)": "<rootDir>/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": ".+/dist/main/tests/.+spec.js$",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "dist/main/src/**/*.js",
      "!dist/main/src/gen/**/*.js",
      "!src/*.d.ts",
      "!src/**/*.d.ts",
      "!src/**/*.spec.ts"
    ]
  }
}
